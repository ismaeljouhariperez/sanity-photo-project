{
  "name": "sanity-photo-project",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "dev": "pnpm --parallel -r dev",
    "dev:next": "pnpm --filter next dev",
    "dev:sanity": "pnpm --filter sanity dev",
    "build": "pnpm -r build",
    "build:next": "pnpm --filter next build",
    "build:sanity": "pnpm --filter sanity build",
    "start": "pnpm --filter next start",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "devDependencies": {
    "prettier": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}